{
  "name": "bucketsvg",
  "version": "1.0.11",
  "description": "BucketSVG a free svg icon library",
  "keywords": [
    "React",
    "Icons",
    "SVG"
  ],
  "main": "dist/src/index.js",
  "module": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.d.ts",
    "dist/index.ts"
  ],
  "scripts": {
    "prebuild": "rimraf src && rimraf dist",
    "build": "npm run svgo && npm run svgr && rollup -c",
    "svgr": "svgr -d src optimized --svgo-config svgo.config.js --title-prop --typescript",
    "svgo": "rimraf ./optimized && svgo -f ./assets -o ./optimized --pretty --indent=2"
  },
  "author": {
    "name": "Rahul Krishnakumar",
    "email": "rahul2381994@gmail.com",
    "url": "https://rahulkrishnakumar.com"
  },
  "repository": "https://github.com/rahulkrishnakumar/bucketsvg",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-typescript": "^8.5.0",
    "@svgr/cli": "^6.3.1",
    "@types/react": "^18.0.21",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-filesize": "^9.1.2",
    "svgo": "^2.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.0"
  }
}
